{
  "build_commands": {
    "1": {
      "name": "Build!",
      "shell_cmd": "nim c -d:release -r ./compileOutput.nim",
      "cwd": ".",
      "autoopen": true
    },
    "2": {
      "name": "Build Debug!",
      "shell_cmd": "nim c -r compileOutput",
      "cwd": ".",
      "autoopen": true
    },
    "3": {
      "name": "Sphinx-build",
      "shell_cmd": "sphinx-build -b html -j auto . _build",
      "cwd": ".",
      "autoopen": true,
      "autoopen_ongoing": true
    },
    "4": {
      "name": "Build and sphinx-build",
      "shell_cmd": "nim c -d:release -r ./compileOutput.nim; sphinx-build -b html -j auto . _build",
      "cwd": ".",
      "autoopen": true,
      "autoopen_ongoing": true
    }
  }
}
